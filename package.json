{
  "name": "lexis-cs",
  "version": "1.3.0",
  "description": "Lexical services for client side scripts",
  "main": "dist/dev/service.js",
  "scripts": {
    "test": "jest --coverage",
    "set-node-build-deps": "npx install-peerdeps alpheios-node-build --dev --only-peers",
    "build": "npm run build-dev && npm run build-prod",
    "build-prod": "npm run lint && node --experimental-modules ./node_modules/alpheios-node-build/dist/build.mjs -m webpack -M production -p lib -c config.mjs",
    "build-dev": "node --experimental-modules ./node_modules/alpheios-node-build/dist/build.mjs -m webpack -M development -p lib -c config.mjs",
    "lint": "eslint --fix ./src/**/*.js",
    "lint-nofix": "eslint ./src/**/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alpheios-project/lexis-cs.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/alpheios-project/lexis-cs/issues"
  },
  "homepage": "https://github.com/alpheios-project/lexis-cs#readme",
  "engines": {
    "node": ">= 13.2.0",
    "npm": ">= 6.9.0"
  },
  "jest": {
    "verbose": true,
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "!node_modules/"
    ],
    "moduleNameMapper": {
      "^@lexisCs[/](.+)": "<rootDir>/src/$1",
      "^@lexisCsCedict[/](.+)": "<rootDir>/node_modules/alpheios-cedict/test/data/$1"
    },
    "moduleFileExtensions": [
      "js"
    ]
  },
  "eslintConfig": {
    "extends": [
      "standard",
      "plugin:jsdoc/recommended",
      "plugin:vue/essential"
    ],
    "env": {
      "browser": true,
      "node": true
    },
    "parserOptions": {
      "parser": "babel-eslint",
      "ecmaVersion": 2019,
      "sourceType": "module",
      "allowImportExportEverywhere": true
    },
    "rules": {
      "no-prototype-builtins": "off",
      "dot-notation": "off"
    }
  },
  "eslintIgnore": [
    "**/dist",
    "**/support"
  ],
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/plugin-transform-modules-commonjs": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "@babel/register": "^7.8.6",
    "@babel/runtime": "^7.8.7",
    "alpheios-fixtures": "github:alpheios-project/fixtures",
    "alpheios-messaging": "github:alpheios-project/alpheios-messaging#i9-lexiscs-lazy-loading",
    "alpheios-node-build": "github:alpheios-project/node-build#v2",
    "archiver": "^3.1.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-dynamic-import-node": "^2.3.0",
    "chalk": "^3.0.0",
    "command-line-args": "^5.1.1",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsdoc": "^22.0.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.2.2",
    "eslint-scope": "^5.0.0",
    "fake-indexeddb": "^3.0.0",
    "fibers": "^4.0.2",
    "fs-extra": "^8.1.0",
    "git-branch": "^2.0.1",
    "imagemin": "^7.0.1",
    "imagemin-jpegtran": "^6.0.0",
    "imagemin-optipng": "^7.1.0",
    "imagemin-svgo": "^7.1.0",
    "inspectpack": "^4.4.0",
    "jest": "^25.1.0",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "path": "^0.12.7",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-safe-important": "^1.2.0",
    "postcss-scss": "^2.0.0",
    "sass": "^1.26.3",
    "sass-loader": "^8.0.2",
    "source-map-loader": "^0.2.4",
    "url-loader": "^3.0.0",
    "vue-loader": "^15.9.0",
    "vue-svg-loader": "^0.16.0",
    "vue-template-compiler": "^2.6.11",
    "vue-template-loader": "^1.0.0",
    "webpack": "^4.42.0",
    "webpack-bundle-analyzer": "^3.6.1",
    "webpack-cleanup-plugin": "^0.5.1",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {}
}
